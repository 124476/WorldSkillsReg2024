<Window x:Class="_1_2_4_Session.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1_2_4_Session"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="800">
    <Grid>
        <StackPanel>
            <Button Name="Back" Content="Назад" FontSize="17" 
                    Click="Back_Click" Width="200" Margin="0,10,50,10" HorizontalAlignment="Right"></Button>
            <Frame ContentRendered="MyFrame_ContentRendered"
                Name="MyFrame" NavigationUIVisibility="Hidden"></Frame>
        </StackPanel>
    </Grid>
</Window>
